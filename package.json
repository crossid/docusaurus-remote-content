{
  "name": "@crossid/docusaurus-remote-content",
  "version": "0.1.1",
  "description": "A plugin for Docusaurus to fetch content from remote sources.",
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "repository": "https://github.com/crossid/docusaurus-plugin-remote-content.git",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc",
    "release": "release-it --github.release --ci",
    "prettier": "prettier --write **/*.{js,jsx,ts,tsx,md,yml,json}"
  },
  "keywords": [
    "docusaurus",
    "plugin",
    "remote"
  ],
  "author": "crossid.io",
  "license": "MIT",
  "prettier": {
    "semi": true,
    "trailingComma": "all",
    "singleQuote": true,
    "printWidth": 80,
    "tabWidth": 2
  },
  "devDependencies": {
    "@docusaurus/types": "^2.0.0-beta.6",
    "@docusaurus/utils-validation": "^2.0.0-beta.6",
    "release-it": "^14.11.5",
    "typescript": "^4.4.2"
  },
  "dependencies": {
    "axios": "^0.21.4",
    "pretty-ms": "^7.0.1"
  }
}
